{
	"name": "basic-voice-bot",
	"version": "1.0.0",
	"description": "A proof-of-concept basic voice bot for @discordjs/voice",
	"main": "dist/bot.js",
	"scripts": {
		"start": "npm run build && node dist/bot.js",
		"lint": "prettier --cache --check . && eslint src --ext mjs,js,ts --cache",
		"format": "prettier --cache --write . && eslint src --ext mjs,js,ts --fix --cache",
		"build": "rimraf dist && tsc",
		"build:check": "tsc --noEmit --incremental false"
	},
	"keywords": [
		"discord",
		"radio",
		"bot",
		"audio",
		"speakers",
		"hardware",
		"dj"
	],
	"author": "",
	"license": "UNLICENSE",
	"dependencies": {
		"@discordjs/voice": "^0.17.0",
		"discord.js": "^14.15.3",
		"ffmpeg-static": "^5.2.0",
		"libsodium-wrappers": "^0.7.10",
		"prism-media": "^2.0.0-alpha.0",
		"youtubei": "^1.4.2",
		"ytdl-core": "^4.11.5"
	}
}
